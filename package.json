    "faye-websocket": "^0.11.3",
    "highlight.js": "^9.18.1",
    "http-proxy": "^1.18.1",
    "image-size": "^0.6.3",
    "jsdom": "^11.12.0",
    "lodash": "^4.17.15",
    "marked": "^1",
    "morgan": "^1.10.0",
    "node-static": "^0.7.11",
